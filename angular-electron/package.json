{
  "name": "metaheuristic-app",
  "version": "0.0.1",
  "main": "main.js",
  "scripts": {
    "ng": "ng",
    "start-electron": "electron .",
    "start": "ng serve",
    "start-disable-host-check": "ng serve --disable-host-check",
    "start-dev": "ng serve --configuration development --disable-host-check --host=localhost",
    "build": "ng build",
    "build2": "ng build --baseHref=/aiai-app/ --prod=true",
    "build3": "ng build --prod=true",
    "build4": "ng build --aot --configuration production",
    "build4.1 ng build --aot --configuration production-no-ssl": "ng build --aot --configuration production-no-ssl",
    "build5": "ng build --aot",
    "build6": "ng build --configuration development",
    "build8": "npm install -g npm@9.4.2",
    "update1": "ng update @angular/core@15 @angular/cli@15",
    "update2": "ng update @angular/core @angular/cli",
    "update3": "ng update @angular/material@11",
    "update4": "npm update --all --force",
    "npm-install-g": "npm install -g @angular/cli@latest",
    "npm-install": "npm install",
    "npm-install-force": "npm install --force",
    "ng add @angular/material": "ng add @angular/material",
    "npm-audit": "npm audit",
    "npm-audit-fix": "npm audit fix",
    "npm-audit-fix-force": "npm audit fix --force",
    "npm-install2": "npm install -g npm",
    "npm-i-package-lockg": "npm i --package-lock-only",
    "ng add @angular-eslint/schematics": "ng add @angular-eslint/schematics",
    "ng g @angular-eslint/schematics:convert-tslint-to-eslint": "ng g @angular-eslint/schematics:convert-tslint-to-eslint metaheuristic-app-e2e",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "version": "ng version",
    "extract-i18n": "ngx-translate-extract  -p /**/*.html /**/*.ts /**/*.pug --input ./src --output ./src/assets/i18n/EN.json ./src/assets/i18n/RU.json --clean --sort --format json --marker marker",
    "ng--migrate-only--from12.0.0 --to 13.0.0": "ng update @angular/cdk --migrate-only --from 12.0.0 --to 13.0.0",
    "sass-migrator namespace **/*.scss": "sass-migrator -n namespace **/*.scss",

    "npm install electron-builder --save-dev": "npm install electron-builder --save-dev",
    "npm install -g electron-packager --save-dev": "npm install -g electron-packager --save-dev",
    "electron-packager": "electron-packager appdirectory metaheuristic --platform=win32 --arch=x64 --electron-version=25.1.0",
    "electron-packager 2": "electron-packager appdirectory metaheuristic --overwrite --asar --electron-version=25.1.0 --platform=win32 --arch=x64 --prune=true --out=release-builds --icon=./build/icon.ico"
  },
  "engines": {
    "node": "v16.20.0",
    "npm": "^9.4.2"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.2.9",
    "@angular/cdk": "^15.2.9",
    "@angular/common": "^15.2.9",
    "@angular/compiler": "^15.2.9",
    "@angular/core": "^15.2.9",
    "@angular/flex-layout": "^15.0.0-beta.42",
    "@angular/forms": "^15.2.9",
    "@angular/material": "^15.2.9",
    "@angular/platform-browser": "^15.2.9",
    "@angular/platform-browser-dynamic": "^15.2.9",
    "@angular/router": "^15.2.9",
    "@biesbjerg/ngx-translate-extract-marker": "1.0.0",
    "@ngx-translate/core": "^14.0.0",
    "@ngx-translate/http-loader": "7.0.0",
    "core-js": "3.30.1",
    "file-saver": "2.0.5",
    "jszip": "3.10.1",
    "normalize.css": "8.0.1",
    "rxjs": "7.8.1",
    "tslib": "^2.5.0",
    "zone.js": "0.13.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.2.8",
    "@angular-eslint/builder": "^15.2.1",
    "@angular-eslint/eslint-plugin": "^15.2.1",
    "@angular-eslint/eslint-plugin-template": "^15.2.1",
    "@angular-eslint/schematics": "^15.2.1",
    "@angular-eslint/template-parser": "^15.2.1",
    "@angular/cli": "^15.2.5",
    "@angular/compiler-cli": "^15.2.9",
    "@angular/language-service": "^15.2.9",
    "@biesbjerg/ngx-translate-extract": "^7.0.4",
    "@types/node": "^16.18.23",
    "@typescript-eslint/eslint-plugin": "^5.60.0",
    "@typescript-eslint/parser": "^5.60.0",
    "electron": "^25.1.0",
    "electron-builder": "^24.4.0",
    "eslint": "^8.39.0",
    "ts-node": "10.9.1",
    "typescript": "4.9.5"
  }
}
